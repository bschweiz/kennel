  })\n            .then(getCustomers)\n    }\n\n    /*\n        You return a context provider which has the\n        `customers` state, the `addCustomer` function,\n        and the `getCustomer` function as keys. This\n        allows any child elements to access them.\n    */\n    return (\n        <CustomerContext.Provider value={{\n            customers, addCustomer, getCustomers\n        }}>\n            {props.children}\n        </CustomerContext.Provider>\n    )\n}","/Users/bschweiz/workspace/kennels/src/components/employee/EmployeeList.js",["111"],"import React, { useContext, useEffect } from \"react\"\nimport { EmployeeContext } from \"./EmployeeProvider\"\nimport { Link } from \"react-router-dom\"\nimport \"./Employee.css\"\n\nexport const EmployeeList = props => {\n    const { employees, getEmployees } = useContext(EmployeeContext)\n\n    useEffect(() => {\n        getEmployees()\n    }, [])\n\n    return (\n        <div className=\"employees\">\n            <h1>Employees</h1>\n\n            <button onClick={() => props.history.push(\"/employees/create\")}>\n                Add Employee\n            </button>\n\n            <article className=\"employeeList\">\n                {\n                    employees.map(employee => {\n                        return <Link key={employee.id} to={`/employees/${employee.id}`}>\n                            <h3>{employee.name}</h3>\n                        </Link>\n                    })\n                }\n            </article>\n        </div>\n    )\n}","/Users/bschweiz/workspace/kennels/src/components/nav/NavBar.js",[],"/Users/bschweiz/workspace/kennels/src/components/ApplicationViews.js",[],"/Users/bschweiz/workspace/kennels/src/components/employee/EmployeeProvider.js",[],"/Users/bschweiz/workspace/kennels/src/components/customer/Customer.js",[],"/Users/bschweiz/workspace/kennels/src/components/employee/Employee.js",[],"/Users/bschweiz/workspace/kennels/src/components/employee/EmployeeForm.js",["112"],"import React,